{
  "name": "web-server",
  "default_attributes": {
  },
  "json_class": "Chef::Role",
  "run_list": [
    "recipe[git]",
    "recipe[apt]",
    "recipe[php]",
    "recipe[php::module_curl]",
    "recipe[php::module_gd]",
    "recipe[php::module_mysql]",
    "recipe[openssl::default]",
    "recipe[imagemagick]",
    "recipe[apache2]",
    "recipe[apache2::mod_expires]",
    "recipe[apache2::mod_php5]",
    "recipe[apache2::mod_rewrite]",
    "recipe[apache2::mod_ssl]"
  ],
  "description": "",
  "chef_type": "role",
  "override_attributes": {
    "apache": {
      "prefork": {
        "maxspareservers": 5,
        "minspareservers": 5,
        "serverlimit": 400,
        "startservers": 5,
        "maxclients": 150,
        "maxrequestsperchild": 0
      },
      "listen_ports": [
        "80",
        "443"
      ]
    }
  }
}
